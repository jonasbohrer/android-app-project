<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.abc.trabEmbarcados">

    <application
        android:allowBackup="true"
        android:icon="@drawable/diet"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.abc.trabEmbarcados.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.abc.trabEmbarcados.DisplayMessageActivity"
            android:parentActivityName=".MainActivity"></activity>
        <activity android:name="com.abc.trabEmbarcados.LogActivity"
            android:parentActivityName=".MainActivity"></activity>
        <activity android:name="com.abc.trabEmbarcados.GraphActivity"
            android:parentActivityName=".MainActivity"></activity>
    </application>

</manifest>